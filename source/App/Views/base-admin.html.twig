{% extends 'base.html.twig' %}
{% block page_specific_styles %}
    <style>
        .sidebar-brand-icon {
            height: 115px;
            width: 115px;
        }
    </style>
{% endblock %}
{% block body %}
<body class="layout-app layout-sticky-subnav ">
{% include 'includes/preloader.html.twig' %}
<div class="mdk-drawer-layout js-mdk-drawer-layout" data-push data-responsive-width="992px">
    <div class="mdk-drawer-layout__content page-content">
        {% include 'includes/admin/header-admin.html.twig' %}
        <div class="border-bottom-2 py-32pt position-relative z-1 bg-gradient-purple">
            <div class="container-fluid page__container d-flex flex-column flex-md-row align-items-center text-center text-sm-left">
                {% block breadcrumb %}
                {% endblock %}
            </div>
        </div>
        {% block content %}
        {% endblock %}
        {% include 'includes/footer.html.twig' %}
    </div>
    {% include 'includes/admin/drawer-admin.html.twig' %}
</div>
{% include 'includes/scripts.html.twig' %}
{% block page_specific_scripts %}
{% endblock %}
</body>
{% endblock %}